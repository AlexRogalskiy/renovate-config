{
  "name": "@artsy/renovate-config",
  "description": "Arty's Renovate preset configs",
  "author": "zephraph <zephraph@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/artsy/renovate-config",
  "repository": "https://github.com/artsy/reaction.git",
  "bugs": {
    "url": "https://github.com/artsy/renovate-config/issues"
  },
  "version": "1.0.1",
  "scripts": {
    "semantic-release": "semantic-release",
    "test": "jest"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "renovate": "13.128.1",
    "semantic-release": "^15.12.0"
  },
  "renovate-config": {
    "default": {
      "extends": [
        "config:base"
      ],
      "packageRules": [
        {
          "packagePatterns": [
            "*"
          ],
          "excludePackagePatterns": [
            "^@artsy"
          ],
          "enabled": false
        }
      ],
      "enabledManagers": [
        "npm"
      ],
      "vulnerabilityAlerts": {
        "enabled": false
      },
      "prHourlyLimit": 0
    }
  },
  "release": {
    "analyzeCommits": {
      "preset": "ember",
      "releaseRules": [
        {
          "tag": "DOC",
          "release": "patch"
        },
        {
          "tag": "FIX",
          "release": "patch"
        },
        {
          "tag": "PATCH",
          "release": "patch"
        },
        {
          "tag": "FEATURE",
          "release": "minor"
        },
        {
          "tag": "BREAKING",
          "release": "major"
        }
      ]
    }
  }
}
